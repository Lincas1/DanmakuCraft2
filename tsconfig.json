{
  "compilerOptions": {
    "lib": [
      "dom",
      "es2015",
      "es2016",
      "es2017"
    ],
    "module": "commonjs",
    "target": "es5",
    "moduleResolution": "node",
    "outDir": "build",
    "declaration": false,
    "allowJs": false,
    "sourceMap": true,
    "inlineSources": false,
    "strict": true,
    "downlevelIteration": true
  },
  "include": [
    "client/src/**/*",
    "scripts/**/*.ts"
  ]
}
